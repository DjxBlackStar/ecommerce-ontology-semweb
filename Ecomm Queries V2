PREFIX ecomm: <http://www.semanticweb.org/ecommerce#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

SELECT ?product ?name ?price
WHERE {
  ?product rdf:type/rdfs:subClassOf* ecomm:Product .
  ?product ecomm:productName ?name .
  ?product ecomm:hasPrice ?price .
}

PREFIX ecomm: <http://www.semanticweb.org/ecommerce#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

SELECT ?product (STR(?name) AS ?cleanName) (STR(?price) AS ?cleanPrice)
WHERE {
  ?product rdf:type/rdfs:subClassOf* ecomm:Product .
  ?product ecomm:productName ?name .
  ?product ecomm:hasPrice ?price .
}
--------------------------------------------------------------------

PREFIX ecomm: <http://www.semanticweb.org/ecommerce#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

SELECT ?product ?name
WHERE {
  ?product rdf:type/rdfs:subClassOf* ecomm:Product .
  ?product ecomm:hasCategory ecomm:Electronics .
  ?product ecomm:productName ?name .
}

PREFIX ecomm: <http://www.semanticweb.org/ecommerce#>

SELECT ?order ?date ?total
WHERE {
  ?order ecomm:purchasedBy ecomm:user1123 .
  ?order ecomm:hasDate ?date .
  ?order ecomm:totalPrice ?total .
}

PREFIX ecomm: <http://www.semanticweb.org/ecommerce#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

SELECT ?product ?productName ?rating ?reviewText
WHERE {
  ?product ecomm:hasReview ?review .
  ?product ecomm:productName ?productName .
  ?review ecomm:hasRating ?rating .
  ?review ecomm:reviewText ?reviewText .
}

PREFIX ecomm: <http://www.semanticweb.org/ecommerce#>

SELECT ?product ?name ?rating
WHERE {
  ?product ecomm:hasReview ?review .
  ?product ecomm:productName ?name .
  ?review ecomm:hasRating ?rating .
  FILTER (?rating > 4.5)
}
